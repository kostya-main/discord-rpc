    },
    "devDependencies": {
        "@sapphire/ts-config": "^5.0.1",
        "@types/node": "^16",
        "@types/ws": "^8.5.12",
        "rimraf": "^6.0.1",
        "typescript": "^5.5.4",
        "esbuild": "0.24.0"
    },
    "engines": {
        "node": ">=16.11.0"
